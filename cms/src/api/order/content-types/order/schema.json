{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "phone_number": {
      "type": "integer",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "zip_code": {
      "type": "string",
      "required": true
    },
    "hear_about_us": {
      "type": "enumeration",
      "enum": [
        "Internet search",
        "Gift certificate"
      ]
    },
    "tips_via_email": {
      "type": "boolean",
      "default": true
    },
    "first_exp": {
      "type": "boolean",
      "default": true
    },
    "description": {
      "type": "text"
    },
    "upload_image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
